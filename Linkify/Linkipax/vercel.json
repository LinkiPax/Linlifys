{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "installCommand": "npm install --force",
        "outputDirectory": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/.*",
      "dest": "dist/index.html"
    }
  ]
}